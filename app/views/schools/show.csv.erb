<% require 'csv' %>
<%= CSV.generate do |csv|
  csv << [nil]+(1...13).map{|m| Date::MONTHNAMES[m]}
  MonthlyStat::VALID_NAMES.each do |name|
    csv << [t("monthly_stat.names.#{name}")] + (1...13).map do |month|
      monthly_stat = @school_monthly_stats[name][month]
      print_value(monthly_stat)
    end
  end
end -%>
