<div id="wrapper">
  <div id="main">
  <div id="content">
    <div class="snapshort-holder">
      <div class="heading">
        <h1>Monthly Snapshot</h1>
        <div class="date-area">
          <div class="refresh"> <a href="#" class="arrow">arrow</a> </div>
          <div class="date"> <a href="#" class="previ"></a> <span class="year">2014</span> <a href="#" class="next"></a> </div>
        </div>
      </div>
      <div class="four-column">
        <div class="column">
          <div class="img-holder"> <a href="#" > <%= image_tag("images/icon1.png", height: '84', width: '86')%></a> </div>
          <div class="text-area"> <span class="num">2564</span> <span class="text">Emails & Contacts</span> </div>
        </div>
        <div class="column">
          <div class="img-holder"> <a href="#" ><%= image_tag("images/icon2.png", height: '84', width: '86')%></a> </div>
          <div class="text-area"> <span class="num">2000</span> <span class="text">Calls Received</span> </div>
        </div>
        <div class="column">
          <div class="img-holder"> <a href="#" ><%= image_tag("images/icon3.png", height: '84', width: '86')%></a> </div>
          <div class="text-area"> <span class="num">Visits 1860</span> <span class="text">Non-Profile  ( 189 ) Profile  ( 1600 )</span> </div>
        </div>
        <div class="column">
          <div class="img-holder"> <a href="#" ><%= image_tag("images/icon4.png", height: '84', width: '86')%></a> </div>
          <div class="text-area"> <span class="num">1129</span> <span class="text">Subscriptions</span> </div>
        </div>
      </div>
      <div class="two-column">
        <div class="column">
          <div class="heading-area">
            <h2>Convertion Rate</h2>
          </div>
          <div class="percentage"> <span class="num">40%</span> <span class="text">Remote Contacts Became Visit</span> </div>
          <div class="percentage"> <span class="num">20%</span> <span class="text">Visit Became Subscriptions </span> </div>
          <div class="percentage"> <span class="num">10%</span> <span class="text">Total Successful Rate</span> </div>
        </div>
        <div class="column2">
          <div class="heading-area">
            <h2>Total demand</h2>
          </div>
          <div class="price-bar">
            <p>
              <input type="text" id="amount" class="value">
              <label for="amount">Total Demand</label>
            </p>
            <div id="slider-range" style="width:210px;"> </div>
          </div>
        </div>
      </div>
    </div>  
  </div>
</div>
